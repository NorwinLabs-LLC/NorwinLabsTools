<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/FirstFragment">

    <fragment
        android:id="@+id/FirstFragment"
        android:name="com.example.norwinlabstools.FirstFragment"
        android:label=""
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_SettingsFragment"
            app:destination="@id/SettingsFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_HapticTesterFragment"
            app:destination="@id/HapticTesterFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_CalendarFragment"
            app:destination="@id/CalendarFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_NetScannerFragment"
            app:destination="@id/NetScannerFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_DevNewsFragment"
            app:destination="@id/DevNewsFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_BugReportFragment"
            app:destination="@id/BugReportFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_BudgetFragment"
            app:destination="@id/BudgetFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_SystemDashboardFragment"
            app:destination="@id/SystemDashboardFragment" />
        <action
            android:id="@+id/action_FirstFragment_to_PortScannerFragment"
            app:destination="@id/PortScannerFragment" />
    </fragment>
    
    <fragment
        android:id="@+id/SecondFragment"
        android:name="com.example.norwinlabstools.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">
        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>

    <fragment
        android:id="@+id/SettingsFragment"
        android:name="com.example.norwinlabstools.SettingsFragment"
        android:label="@string/settings_fragment_label"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/HapticTesterFragment"
        android:name="com.example.norwinlabstools.HapticTesterFragment"
        android:label="Haptic Tester"
        tools:layout="@layout/fragment_haptic_tester" />

    <fragment
        android:id="@+id/CalendarFragment"
        android:name="com.example.norwinlabstools.CalendarFragment"
        android:label="Calendar"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/NetScannerFragment"
        android:name="com.example.norwinlabstools.NetScannerFragment"
        android:label="Net Scanner"
        tools:layout="@layout/fragment_net_scanner" />

    <fragment
        android:id="@+id/DevNewsFragment"
        android:name="com.example.norwinlabstools.DevNewsFragment"
        android:label="Dev &amp; Accessibility News"
        tools:layout="@layout/fragment_dev_news" />

    <fragment
        android:id="@+id/BugReportFragment"
        android:name="com.example.norwinlabstools.BugReportFragment"
        android:label="Report a Bug"
        tools:layout="@layout/fragment_bug_report" />

    <fragment
        android:id="@+id/BudgetFragment"
        android:name="com.example.norwinlabstools.BudgetFragment"
        android:label="Budget"
        tools:layout="@layout/fragment_budget" />

    <fragment
        android:id="@+id/SystemDashboardFragment"
        android:name="com.example.norwinlabstools.SystemDashboardFragment"
        android:label="System Dashboard"
        tools:layout="@layout/fragment_system_dashboard" />

    <fragment
        android:id="@+id/PortScannerFragment"
        android:name="com.example.norwinlabstools.PortScannerFragment"
        android:label="Port Scanner"
        tools:layout="@layout/fragment_port_scanner" />

</navigation>